<Window x:Class="harjoitus.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:harjoitus"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox Text="{Binding Path = Numero}" Margin="57,47,328.4,219.8"/>
        <Button Name="AButton" Click="OnImageButtonClick" Margin="234,145,43.4,9.8" Background="{x:Null}" BorderBrush="{x:Null}">
            <Image Source="{Binding Image}" Width="232" Height="87"></Image>
        </Button>
    </Grid>
</Window>
